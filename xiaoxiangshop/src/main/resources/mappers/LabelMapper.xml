<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.xiaoxiangshop.demos.dao.ILabelDao">

    <select id="findLabelByPid" resultType="label">
        select t2.* from label_details t1,label t2 where t1.product_id = #{id} and t1.label_id = t2.label_id
    </select>
</mapper>
